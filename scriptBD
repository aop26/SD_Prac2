

create table if not exists  RIDE
    ( id integer primary key autoincrement,
    x integer, 
    y integer, 
    maxPeople integer,
    waitTime integer
);



create table if not exists CLIENT
    (id integer primary key autoincrement,
    x integer, 
    y integer,  
    username varchar2 not null, 
    password varchar2 not null,
    constraint unique_userName unique(username)
);



insert into RIDE(x,y,waitTime,maxPeople) VALUES(12, 7, 1,10);
insert into RIDE(x,y,waitTime,maxPeople) VALUES(1, 11, 2,20);
insert into RIDE(x,y,waitTime,maxPeople) VALUES(17, 2, 3,30);
insert into RIDE(x,y,waitTime,maxPeople) VALUES(5, 7, 4,40);

